= link_to 'Back', root_path

%tbody
  - @produits.each do |produit|
    %tr
      %td= produit.nom
      %td= produit.disponibilite
      %td= produit.description
      %td= produit.image
      %td= link_to 'Show', produit
      -if(produit.disponibilite)
        %td= link_to 'Locate', produit_locate_path(produit)
      -else
        %p Not available